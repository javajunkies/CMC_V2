import UniversityDBController;
import System.out.*;

public class AccountController implements DBController {
  
  public void editUser(String firstName, String lastName, String username, String password) {
    Account user = DBController.findByUsername(username);
    if (DBController.passwordSecurity()) {
      user.setFirst(firstName);
      user.setLast(lastName);
    }
  }
  
